<section class="uk-section uk-background-muted">
    <div class="uk-container">
        <div class="uk-grid-match uk-grid-small" uk-grid>
            <div class="uk-width-2-3">
                <div class="uk-card uk-card-default uk-padding uk-box-shadow-large uk-border-rounded">
                    <h4>My Student Card</h4>
                    <div class="uk-inline uk-flex">
                        <div class="uk-card-media-top uk-width-1-3">
                            {{#if user.image}}
                            <img src="{{user.image}}" alt="{{user.name}}">
                            {{else}}
                            <img class="uk-border-circle uk-padding-small"
                                data-src="https://cdn.pixabay.com/photo/2015/10/05/22/37/blank-profile-picture-973460_960_720.png"
                                width="200" height="200" alt="profile-image" uk-img>
                            {{/if}}
                            <div class="uk-card-body">
                                <h5 class="uk-card-title uk-text-primary">{{user.name}} {{user.lastname}}</h5>
                                <p>{{user.skill1}}</p>
                                <span uk-icon="world" class="uk-padding-xsmall" value="{{user.skill18}}"></span>
                                <span uk-icon="linkedin" class="uk-padding-xsmall" value="{{user.skill19}}"></span>
                                <span uk-icon="github" class="uk-padding-xsmall" value="{{user.skill20}}"></span>
                                <span uk-icon="behance" class="uk-padding-xsmall" value="{{user.skill21}}"></span>
                                <span uk-icon="user" class="uk-padding-xsmall" value="{{user.skill22}}"></span>

                            </div>
                            <a  class="uk-width-1-1 uk-button uk-button-primary uk-border-rounded uk-box-shadow-small uk-margin-small-top" href="/sboard"> Edit your card</a>
                            <div class="uk-margin">
                                <a class="uk-link-text" href="/outcomes-view">Search Students</a> </div>
                        </div>
                        <hr class="uk-divider-vertical">

                        <div class="uk-width-expand uk-padding-small uk-align-center uk-align-right@m"
                            uk-container-expand>
                            <h5>ABOUT ME</h5>
                            <strong>Why did you do Ironhack?</strong>
                            <li>{{user.skill2}}</li>
                            <strong>What do you look for in company culture?</strong>
                            <li>{{user.skill3}}</li>
                            <strong>Superpower and why?</strong>
                            <li>{{user.skill4}}</li>

                            <h5>MY PROJECTS</h5>
                            <strong>Name of Project 1</strong>
                            <li>{{user.kill5}}</li>
                            <strong>Name of Project </strong>
                            <li>{{user.kill6}}</li>
                            <strong>Name of Project 1</strong>
                            <li>{{user.kill7}}</li>

                            <h5>MY SOFT SKILLS</h5>
                            <li>{{user.kill8}}</li>
                            <li>{{user.kill9}}</li>
                            <li>{{user.kill10}}</li>
                            <li>{{user.kill11}}</li>
                            <li>{{user.kill12}}</li>

                            <h5>HARD SKILLS / TECHNOLOGIES</h5>
                             <li>{{user.kill13}}</li>
                            <li>{{user.kill14}}</li>
                            <li>{{user.kill15}}</li>
                            <li>{{user.kill16}}</li>
                            <li>{{user.kill17}}</li>

                        </div>
                    </div>
                </div>
            </div>

